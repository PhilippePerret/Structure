<div id="stt-editingstt" data-actif="false" class="hidden">
  <div class="stt-header">
    <h2>Structure éditable</h2>
  </div>
  <div id="stt-editingstt-listing">

      <!-- Élément par défaut qui va être cloné -->

      <div class="sttediting-element" data-id="">

        <input type="hidden" class="elt-id" />
        <input type="text" class="elt-time horloge">
        <input type="text" class="elt-duree horloge">
        <.select_type id={nil} class={"elt-type"} />
        <.select_ideality id={nil} class={"elt-ideality"} />
        <input type="text" class="elt-pitch" style="width:500px;">
        <select class="elt-color"></select>
        <input type="text" class="elt-tension" style="width:100px;">
        <span class="boutons">
          <button class="btn-add" title="Ajouter avant">＋</button>
          <button class="btn-del" title="Supprimer l'élément">🗑️</button>
        </span>
      </div>

  </div>
</div>